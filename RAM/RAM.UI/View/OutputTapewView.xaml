<UserControl
	x:Class="RAM.UI.View.OutputTapewView"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:RAM.UI.View"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DesignHeight="300"
	d:DesignWidth="300"
	DataContext="{Binding OutputTapeViewModel, Source={StaticResource ModelViewLocator}}"
	mc:Ignorable="d">
	<Grid>
		<DataGrid
			AutoGenerateColumns="False"
			ItemsSource="{Binding TapeMembers}"
			SelectedItem="{Binding SelectedTapeMember}">
			<DataGrid.Columns>
				<DataGridTextColumn
					Width="*"
					Binding="{Binding Number}"
					Header="No." />
				<DataGridTextColumn
					Width="4*"
					Binding="{Binding Value}"
					Header="Value" />
			</DataGrid.Columns>
		</DataGrid>
	</Grid>
</UserControl>
